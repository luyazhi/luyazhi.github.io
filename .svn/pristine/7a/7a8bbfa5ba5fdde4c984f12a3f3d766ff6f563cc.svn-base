.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 9000;
  color: #fff;
}
.modal-dialog {
  width: 540rpx;
  height:375rpx;
  overflow: hidden;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 9999;
  background: #f9f9f9;
  margin: auto;


}
.modal-title {
  width: 90%;
  margin-left: 5%;
  padding: 15rpx 0rpx 15rpx 0rpx;
  font-size: 28rpx;
  color: #000;
  text-align: center;
  font-weight: bold;
  border-bottom:1px solid gainsboro; 
}

.modal-content {
  width: 90%;
  margin-left: 5%;
  border-radius: 4rpx;
  font-size: 28rpx;
  height: 200rpx;
  padding-left: 20rpx;

}
.modal-content view{
  width: 100%;
  height: 55rpx;
  line-height: 55rpx;
}

input-holder {
  color: #666;
  font-size: 32rpx;
}
.modal-footer {
  width: 90%;
  margin-left: 5%;
  display: flex;
  flex-direction: row;
  height: 70rpx;
  border-top: 1px solid #dedede;
  font-size: 28rpx;
  line-height: 70rpx;
}
.btn-cancel {
  width: 50%;
  color: #666;
  text-align: center;
  border-right: 1px solid #dedede;
}
.btn-confirm {
  width: 50%;
  color: #ec5300;
  text-align: center;
}


