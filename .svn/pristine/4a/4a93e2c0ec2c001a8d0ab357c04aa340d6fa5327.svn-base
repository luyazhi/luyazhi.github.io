page {
  background:rgba(224, 224, 224, 0.3215686274509804);
}
text,input{color: #fff;}
.person-wrap {
  width: 94%;
  margin: 20rpx auto 20rpx auto;
  min-height: 180rpx;
  background: #fff;
  border-radius: 14rpx;
  padding:40rpx 0;
}
.titleText{
  font-size:36rpx;
  color:#000;
  margin: 0 24px;
  padding:10rpx 0;
  display: block;
}
.audioStyle{
  width: 86%;
  margin: 0 auto;
}
audio{
  position: relative;
}
audio .free-MusicProgress{
  position: absolute;
  width:78%;
  left:21.7%;
  bottom:1px;
  background:#C3C3C3;
}
audio .free-MusicProgress>view{
  background:#48c23d;
  height:2px;
}
.showtText{
  color: #000;
  display:block;
  margin:13rpx 24px;
font-size:26rpx;
line-height:42rpx;
}
image.img1{
  width:86%;
margin:24rpx auto;
display:block;
height:350rpx;

}
.hide{display: none;}
.more{
  display: block;
  text-align: center;
  color: #0084ff;
  font-size:28rpx;
}
.Set_meal{
 margin:0rpx 24px;
background:#e9e9e9;
border:1px solid #ccc;
overflow:hidden;
padding:26rpx 0;

}
.manytext{
  display:-webkit-box;
-webkit-box-orient:vertical;
-webkit-line-clamp:2;
overflow:hidden;
color:#000;
font-size:26rpx;
width:86%;
float:left;
padding:0 0 0 20rpx;
line-height:38rpx;

}
.ico{
 display:block;
