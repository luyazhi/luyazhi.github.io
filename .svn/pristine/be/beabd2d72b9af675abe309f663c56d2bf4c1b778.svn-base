<!--pages/index/charter_flight/charter_flight.wxml-->
<view class="person-wrap">
  <text class="titleText">{{title}}</text>

  <view class="menu_text">
    <image class="img1" src="../../../images/7.png"></image>
    <view class="audioStyle">
      <audio poster="{{poster}}" bindtap="audio" author="{{author}}" id="myAudio" bindplay="funplay" bindtimeupdate="MusicStart" bindended="MusicEnd">
        <image catchtap="onMusicTap" class="audio-img" src="{{imgSrc}}"></image>
        <text style='font-size:26rpx'>总时长：{{audioSumTime > 9 ? audioSumTime : '0'+audioSumTime}}:{{audioSumTimes > 9 ? audioSumTimes : '0'+audioSumTimes}}</text>
        <text style='font-size:26rpx'>当前时间：{{currentAudioTime > 9 ? currentAudioTime : '0'+ currentAudioTime}}:{{currentAudioTimes> 9 ? currentAudioTimes : '0'+ currentAudioTimes}}</text>
        <view class="free-MusicProgress">
          <view style="width:{{progress}}%;" wx:if="{{progress}}"></view>
        </view>
      </audio>
    </view>
    <view class="showtText">桃花潭，位于泾县以西40公里处，景区内自然景观和人文景
      <text>... ...</text>
    </view>
    <text class="more">阅读更多</text>
  </view>
</view>

<view class="person-wraps">
  <text class="titleText">游玩套餐</text>
  <navigator class="Set_meal" url="../reserve/reserve">
    <view style='clear:both;'>
      <view class='Set_meal-img'>
        <image src='../../../images/WechatIMG5219.png' class='Set_meal-img1'></image>
        <image src='../../../images/Group3.png' class='Set_meal-img2'></image>
        <image src='../../../images/Rectangle8Copy6.png' class='Set_meal-img3'></image>
        <image src='../../../images/3DAYSCopy.png' class='Set_meal-img4'></image>

      </view>
      <view class='right'>
        <view class='right-text'>
          <title class="manytitle">探密金秋童话+体验丝路之美...</title>
          <text class="manytext">嘉峪关/额济纳旗/嘉峪关小环线</text>
          <text class="manytext">3日包车游</text>
        </view>
        <view class='right-img'>
          <image src='../../../images/Rectangle2Copy2.png' class='Set_meal-img5'></image>
          <image src='../../../images/copy.png' class='Set_meal-img6'></image>
        </view>
      </view>

    </view>
  </navigator>
  <navigator class="Set_meal Set_meal1" url="../reserve/reserve">
    <view style='clear:both;'>
      <view class='Set_meal-img'>
        <image src='../../../images/WechatIMG5219.png' class='Set_meal-img1'></image>
        <image src='../../../images/Group3Copy.png' class='Set_meal-img2'></image>
        <image src='../../../images/Rectangle8Copy6.png' class='Set_meal-img3'></image>
        <image src='../../../images/3DAYSCopy.png' class='Set_meal-img4'></image>

      </view>
      <view class='right'>
        <view class='right-text'>
          <title class="manytitle">探密金秋童话+体验丝路之美...</title>
          <text class="manytext">嘉峪关/额济纳旗/嘉峪关小环线</text>
          <text class="manytext">3日包车游</text>
        </view>
        <view class='right-img'>
          <image src='../../../images/Rectangle2Copy2.png' class='Set_meal-img5'></image>
          <image src='../../../images/copy.png' class='Set_meal-img6'></image>
        </view>
      </view>

    </view>
  </navigator>
</view>
<view class="person-wrap" style="margin-bottom:100rpx;">
  <text class="titleText">线下服务门店</text>
  <text class="showtText1">我们提供线下专门门店服务，您可以在门店提取车辆享受景区专属接待服务</text>
  <view class="Copy-bottom" style="border:none;">
    <image src='../../../images/Page1Copy2.png' class='Copy-bottom-img1'></image>
    <text class="Copy-bottom-text">门店地址：XX XX XX</text>
    <image class='Copy-bottom-img1' src="../../../images/Page1Copy2.png" bindtap="actionMapTap"></image>
  </view>
  <view class="Copy-bottom" style="border:none;">
    <image src='../../../images/phone(2)copy16.png' class='Copy-bottom-img1' ></image>
    <text class="Copy-bottom-text">服务电话：</text>
    <text class="Copy-bottom-text" style='color:#FCCF00'>400-107-9995</text>
  </view>
</view>
