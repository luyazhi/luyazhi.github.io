

<view style='height:20rpx;'></view>

<view bindtouchstart='own_event' bindtouchmove='own_event' bindtouchend='own_event' bindtouchcancel='own_event' class='view_ul'>
  <!-- <view class="view_ul" bindtouchstart="touchstart" bindtouchmove="touchmove" bindtouchend="touchend"> -->
  
    <image class='image1 img1' src="{{clubs[0].src}}">
      <navigator class='prompt-textwrap {{test_animate}}' bindtap='url_click' >
      <view class="prompt-text">{{clubs[0].name}}</view>
      <view class='prompt-destext'>{{clubs[0].des}}</view>
    </navigator>
  </image>

  <image class='image1 img2 {{test_class}}' bindtap='image_click' src="{{clubs[1].src}}" bindtab="alertSwiper">
    <navigator class='prompt-textwrap {{test_animate}}' bindtap='url_click' >
      <view class="prompt-text">{{clubs[1].name}}</view>
      <view class='prompt-destext'>{{clubs[1].des}}</view>
    </navigator>
  </image>

  <image class='image1 img3 ' src="{{clubs[2].src}}">
      <navigator class='prompt-textwrap {{test_animate}}' bindtap='url_click' >
      <view class="prompt-text">{{clubs[2].name}}</view>
      <view class='prompt-destext'>{{clubs[2].des}}</view>
    </navigator>
  </image>

  

</view>

