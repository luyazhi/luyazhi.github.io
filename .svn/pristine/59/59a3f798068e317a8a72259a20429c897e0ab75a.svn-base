/* pages/index/detail/reserve/reserve.wxss */
@import "indent/indent.wxss";

.person-wrap {
  width: 94%;
  margin: 20rpx auto 20rpx auto;
  min-height: 180rpx;
  background: #fff;
  padding: 40rpx 0;
  border: 1px solid gainsboro;
}

.person-wraps {
  width: 94%;
  margin: 20rpx auto 20rpx auto;
  padding: 40rpx 0;
}

.menu_text {
  text-align: center;
}

.manytitle {
  font-size: 44rpx;
  color: #4a4a4a;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  display: block;
  padding: 0 27rpx;
}

.titleText {
  font-size: 36rpx;
  color: #000;
  margin: 0 24px;
  padding: 10rpx 0;
  display: block;
}

.audioStyle {
  width: 86%;
  margin: 0 auto;
}

.showtText {
  color: #4a4a4a;
  display: block;
  margin: 13rpx 24rpx 0;
  font-size: 26rpx;
  line-height: 40rpx;
}

image.img1 {
  margin: 24rpx auto;
  display: block;
  height: 292rpx;
  width: 100%;
}

.hide {
  display: none;
}

.more {
  display: block;
  text-align: center;
  color: #fdd100;
  font-size: 28rpx;
  padding: 21px 0 0px 0;
  font-weight: bold;
}

.contbox {
  display: flex;
  justify-content: space-between;
  width: 100%;
  height: 70rpx;
  line-height: 40px;
  font-size: 26rpx;
  border-bottom: 1rpx solid #ccc;
}

images.itembox {
  display: block;
  width: 45rpx;
  height: 45rpx;
  border: 1px solid #ccc;
}

.itembox {
  margin: 0px 14px 0 24px;
}

.text1 {
  margin: 0px 14px 0 24px;
  font-size: 28rpx;
  display: block;
  line-height: 50rpx;
  color: #4a4a4a;
  font-weight: bold;
}

.text2 {
  margin: 0px 14px 0 24px;
  font-size: 24rpx;
  display: block;
  color: #9b9b9b;
  line-height: 36rpx;
}

.itembox1 {
  width: 60%;
  margin-left: 16%;
}

.itembox2 {
  width: 80%;
  color: #4a4a4a;
  font-size: 28rpx;
  font-weight: bold;
}

.itembox3 {
  width: 40%;
  display: flex;
  line-height:40rpx;
  margin:16rpx auto;

}

.butstyle {
  line-height:28rpx;
float:left;
font-size:66rpx;
background:none;

}

text.inputstylex {
  display: block;
}

.inputstylex {
display:flex;
text-align:center;
border:none;
height:32px;
font-size:36rpx;


}

.titleTxt {
  color: #000;
  font-size: 24rpx;
  margin: 0 24px;
  width: 90%;
  display: block;
}

.servicebox {
  display: flex;
  width: 100%;
  margin-top: 30rpx;
}

.item {
  width: 30%;
  margin: auto;
  font-size: 20rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

image.img {
  border: 1px solid #ccc;
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
}

.item text {
  font-size: 25rpx;
  text-align: center;
  display: block;
  color: #000;
}

.address_add {
  position: fixed;
  bottom: 0;
  width: 100%;
  background: #fff;
}

.buttonSub {
  width: 40%;
  float: left;
  display: block;
  border-radius: 0;
}

button[plain] {
  color: #fff;
  background-color: #fdd100;
  border: none;
}

.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 9000;
  color: #fff;
}

.modal-log {
  width: 540rpx;
  overflow: hidden;
  position: fixed;
  top: 50%;
  left: 0;
  z-index: 9999;
  background: #f9f9f9;
  margin: -180rpx 105rpx;
  border-radius: 36rpx;
}

.imglist {
  position: relative;
  overflow: hidden;
}

.imgico {
  position: absolute;
  top: 10px;
  left: -14px;
  background: rgba(253, 208, 0, 0.76);
  display: block;
  width: 96px;
  height: 30px;
  border-radius: 26px;
  line-height: 30px;
  text-align: center;
  color: #fff;
  font-size: 30rpx;
}

.dots {
  position:absolute;
bottom:8px;
right:16px;
display:block;
width:40px;
height:17px;
line-height:30px;
text-align:right;
color:#fff;
font-size:30rpx;

}

.dotstext {
 padding:4px 12px;
display:block;
position:relative;
top:-55rpx;
left:-7rpx;

}

.img2ico {
  position: absolute;
  top: 10px;
  left: 0px;
  background: rgba(253, 208, 0, 0.76);
  display: block;
  width: 96px;
  height: 30px;
  border-radius: 26px;
  line-height: 30px;
  text-align: center;
  color: #fff;
  font-size: 30rpx;
}

.pic2 {
  position: absolute;
  top: -13px;
  right: 26px;
  width: 47px;
  height: 75px;
  line-height: 64px;
  text-align: center;
  color: #fff;
  font-size: 36rpx;
  display: block;
  padding-right: 2px;
}

.pic1 {
line-height:13px;
padding-left:22px;
height:30px;
width:176rpx;
position:absolute;
top:26rpx;
left:0;

}
.pic1 text,.pic2 text{
 color:#fff;
position:relative;
top:-46rpx;
font-size:32rpx;
}

.dinzhitxt {
  padding: 0 27px 25px;
  border-bottom: 1px solid #d8d8d8;
}

.pickery {
  font-weight: bold;
  font-size: 29rpx;
  position: relative;
  right: -43px;
}
