.container{
  width: 100%;
  margin: 0 10%;
}
.log-list-title{
  font-size: 38rpx;
}
.cared{
  margin-top:20rpx; 
  font-size: 30rpx;
  width: 70%;
  height: 230rpx;
  border: 1px solid black;
  padding: 5%;
  line-height: 45rpx;
}
.cared-title{
  color: blue;
  
}
.cared-text{
  display: flex;
  justify-content: space-between;
  margin-top: 20rpx;
}
.cared-btn{
  width: 80%;
  display: flex;
  height: 60rpx;
  border: 1px solid black;
}
.cared-btn button{
  margin: 0;
  flex: 1;
  text-align: center;
  line-height: 60rpx;
  font-size: 30rpx;
  border: none;
}
.cared-btns{
  width: 80%;
  height: 60rpx;
  border: 1px solid black;
}
.cared-btns button{
  color: blue;
  height: 60rpx;
  line-height: 60rpx;
}