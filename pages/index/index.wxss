@font-face {
  font-family: 'iconfont'; /* project id 518032 */
  src: url('//at.alicdn.com/t/font_518032_t2q88z3jok8iwwmi.eot');
  src: url('//at.alicdn.com/t/font_518032_t2q88z3jok8iwwmi.eot?#iefix') format('embedded-opentype'),
         url('//at.alicdn.com/t/font_518032_t2q88z3jok8iwwmi.woff') format('woff'),
         url('//at.alicdn.com/t/font_518032_t2q88z3jok8iwwmi.ttf') format('truetype'),
         url('//at.alicdn.com/t/font_518032_t2q88z3jok8iwwmi.svg#iconfont') format('svg');
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 60rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

image {
  height: 800rpx;
  overflow: inherit;
  border-radius: none;
}



.box .club:nth-child(1) {
  transform: scale(1, 1)translateX(0rpx);
  opacity: 0.5;
  z-index: 10;
}

.box .club:nth-child(2) {
  transform: scale(1, 1);
  opacity: 1;
  z-index: 100;
}

.box .club:nth-child(3) {
  transform: scale(1, 1)translateX(0rpx);
  opacity: 0.2;
  z-index: 10;
}

.mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  z-index: 9000;
  opacity: 0.7;
}

.modalDlg {
  width: 580rpx;
  height: 620rpx;
  position: fixed;
  top: 50%;
  left: 0;
  z-index: 9999;
  margin: -370rpx 85rpx;
  background-color: #fff;
  border-radius: 36rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

/*弹出层*/

/*button*/

.btn {
  width: 80%;
  padding: 20rpx 0;
  border-radius: 10rpx;
  text-align: center;
  margin: 40rpx 10%;
  background: #000;
  color: #fff;
}

/*mask*/

.drawer_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 1000;
  background: #000;
  opacity: 0.7;
  overflow: hidden;
}

/*content*/

.drawer_box {
  width: 100%;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 1001;
  margin: 0 auto;
  border-radius: 6rpx;
  height: 400rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.sm_icon {
  position: relative;
}

.sm_icon .ico-text {
  color: #fff;
  position: absolute;
}

.digifrm {
  position: absolute;
  bottom: 40rpx;
  display: flex;
  flex-direction: column;
  left: 40%;
  align-items: center;
  justify-content: center;
  opacity: 0;
}

.icontext {
  width: 100rpx;
  height: 100rpx;
  text-align: center;
  font-size: 200rpx;
}

.animate1 {
  -webkit-transition-duration: 400ms;
  transition-duration: 400ms;
  -webkit-transform: translate3d(-180rpx, -100rpx, 0);
  transform: translate3d(-180rpx, -100rpx, 0);
  opacity: 1;
}

.animate2 {
  -webkit-transition-duration: 400ms;
  transition-duration: 400ms;
  -webkit-transform: translate3d(0rpx, -200rpx, 0);
  transform: translate3d(0rpx, -200rpx, 0);
  opacity: 1;
}

.animate3 {
  -webkit-transition-duration: 400ms;
  transition-duration: 400ms;
  -webkit-transform: translate3d(180rpx, -100rpx, 0);
  transform: translate3d(180rpx, -100rpx, 0);
  opacity: 1;
}

.digifrm text {
  display: block;
  color: #fff;
  text-align: center;
  font-size: 33rpx;
  font-weight: normal;
}




.btn_ok {
  height: 100rpx;
  width: 100rpx;
  margin: 0 auto;
  position: absolute;
  bottom: 20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  left: 0;
  right: 0;
  background: #000;
  border-radius: 50%;
}

.digifrm:nth-child(1) {
  left: 30%;
}

.digifrm:nth-child(3) {
  left: 48%;
}

.animate4 {
  -webkit-transition-duration: 800ms;
  transition-duration: 800ms;
  -webkit-transform: rotate(180deg);
  transform: rotate(180deg);
  opacity: 1;
}

.close_img {
  position: absolute;
  height: 102rpx;
  width: 102rpx;
  clear: both;
}


.title {
  height: 60rpx;
  line-height: 60rpx;
  width: 160rpx;
  text-align: center;
  display: inline-block;
  font: 300 28rpx/30px "microsoft yahei";
}

.input_base {
  border: 2rpx solid #ccc;
  padding-left: 10rpx;
  margin-right: 50rpx;
}

.input_h30 {
  height: 60rpx;
  line-height: 60rpx;
}

.input_h60 {
  height: 120rpx;
}

.input_view {
  font: 24rpx "microsoft yahei";
  background: #fff;
  color: #000;
  line-height: 60rpx;
}

input {
  font: 24rpx "microsoft yahei";
  background: #fff;
  color: #000;
}

radio {
  margin-right: 40rpx;
}

.grid {
  display: -webkit-box;
  display: box;
}

.col-0 {
  -webkit-box-flex: 0;
}

.col-1 {
  -webkit-box-flex: 1;
}

.fl {
  float: left;
}

.fr {
  float: right;
}

.sm_icon {
  width: 100rpx;
  height: 100rpx;
  /* border-radius: 50%; */
  margin: 0 30rpx;
}

.image1 {
  width: 110%;
  height:800rpx;
  position: absolute;
  left: 13%;
  top: -20rpx;
  background-size: cover;
  background-repeat: no-repeat;
  background-position: center;
  transform-origin: 0 50%;
  border-radius: 0rpx;
  opacity: 0.6;
 box-shadow:1rpx 2rpx 5rpx #d6d6d6;
}

.img1 {
  z-index: 40;
  transform: scale(0) translate(-100%, 0);
  opacity: 0;
}

.img2 {
  z-index: 30;
  opacity: 1;
  transform: scale(0.9) translate(0, 0);
}

.img3 {
  z-index: 20;
  transform: scale(0.7) translate(35%, 0);
  opacity: 0.2;
}

.img4 {
  z-index: 10;
  transform: scale(0.6) translate(63%, 0);
  opacity: 0.2;
}

.img5 {
  z-index: 0;
  transform: scale(0.5) translate(100%, 0);
  opacity: 0;
}

@keyframes moveToRight {
  to {
    transform: translateX(-120%);
    opacity: 1;
  }
}

@keyframes moveToLeft {
  to {
    opacity: 0.4;
    transform: scale(0.7) translateX(35%);
  }
}

/* @keyframes moveFromLeft {
	from { transform: translateX(-100%);
  }
}  */

@keyframes scaleUp {
  from {
    opacity: 0.7;
    transform: scale(0.8);
  }
}

@keyframes rotate {
  from {
    transform: rotateX(-90deg);
  }
}

@keyframes scaleBig {
  0% {
    opacity: 1;
    transform: scale(1, 1);
    left: 10%;
  }

  100% {
    opacity: 0.8;
    transform: scale(1.5, 1.5);
    left: -18%;
  }
}

@keyframes moveToTop {
  to {
    transform: translateY(-100%);
  }
}

@keyframes moveToBottom {
  to {
    transform: translateY(100%);
  }
}

