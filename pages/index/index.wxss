  opacity: 1;
}




.view_ul {
  margin-top: -10rpx;
  width: 80%;
  height: 80%;
  list-style-type: none;
  position: relative;
  top: 25rpx;
}

.swiperanimate {
  /* 透明 */
  opacity: 0.8;
  -webkit-animation: scaleBig 0.5s ease-in both;
  -moz-animation: scaleBig 0.5s ease-in both;
  animation: scaleBig 0.5s ease-in both;
}


.pt-page-moveToRight {
  -webkit-animation: moveToRight 0.5s ease both;
  -moz-animation: moveToRight 0.5s ease both;
  animation: moveToRight 0.5s ease both;
}
.pt-page-moveToLeft {
  -webkit-animation: moveToLeft 0.6s ease both;
  -moz-animation: moveToLeft 0.6s ease both;
  animation: moveToLeft 0.6s ease both;
}

@keyframes moveToLeft{
from {left:0px;}
to {left:50px;}
}

@-webkit-keyframes moveToLeft
{
from {left:0px;}
to {left:0;}
}







.footer_backimg {
  position: fixed;
  height: 12%;
  left: 0;
  bottom: 0;
  width: 100%;
  background-image: url("data:image/png;base64,");
  background-repeat: no-repeat;
  background-size: 100% 100%;
  z-index: -999;
}

/* 其实是往左 */

.pt-page-moveToRight +.img3 {
  transition: all 0.45s;
  opacity: 1;
  transform: scale(0.9) translate(0, 0);
}

.pt-page-moveToRight +.img3+.img4 {
  transition: all 0.45s;
  transform: scale(0.7) translate(35%, 0);
}

/* 其实是往右 */

.pt-page-moveToLeft +.img3 {
  transition: all 0.5s;
  opacity: 0.2;
  transform: scale(0.6) translate(63%, 0);
}

.pt-page-moveToLeft+ .img3+.img4 {
  transition: all 0.5s;
  opacity: 0.2;
  transform: scale(0.5) translate(100%, 0);
}


